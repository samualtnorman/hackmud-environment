{
	"compilerOptions": {
		"moduleResolution": "Node",
		"resolveJsonModule": true,
		"noEmit": true,
		"checkJs": false,
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"target": "ESNext",
		"skipLibCheck": true,
		"lib": [ "ES2022" ],
		"types": [ "hackmud-script-manager/env", "@total-typescript/ts-reset" ],
		"strict": true
	}
}
